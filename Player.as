package {		import flash.display.DisplayObjectContainer;	import flash.display.MovieClip;	import flash.geom.Rectangle;		public class Player {				private var mc:MovieClip;				public function Player() {					}				public function intersects ( rect:Rectangle ):Boolean {						return mc.getBounds(mc).intersects(rect);					}				public function addChild ( doc:DisplayObjectContainer ):void {			doc.addChild ( mc );		}	}	}